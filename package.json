    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.3.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.17",
    "lint-staged": "^16.2.7",
